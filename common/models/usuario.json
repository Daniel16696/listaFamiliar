{
  "name": "Usuario",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "apellidos": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "UsuarioPerteneceAListaFamiliar": {
      "type": "belongsTo",
      "model": "ListaFamiliar",
      "foreignKey": ""
    },
    "UsuarioTieneUnListaFamiliar": {
      "type": "hasOne",
      "model": "ListaFamiliar",
      "foreignKey": "Owner"
    },
    "Solicituds": {
      "type": "hasAndBelongsToMany",
      "model": "ListaFamiliar",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "usuario"
    }
  ],
  "methods": {
    "prototype.usuario": {
      "accepts": [
        {
          "arg": "aceptarSoli",
          "type": "object",
          "required": true,
          "description": "Acepta las solicitudes",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": [
        {
          "arg": "SalidaAceptarSolicitud",
          "type": "object",
          "root": true,
          "description": "Acepta las solicitudes"
        }
      ],
      "description": "Creacion",
      "http": [
        {
          "path": "/aceptarSolicitud",
          "verb": "put"
        }
      ]
    }
  }
}
